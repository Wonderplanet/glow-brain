{
  "env": {
    "ENABLE_TOOL_SEARCH": "true"
  },
  "hooks": {
    "UserPromptSubmit": [
      {
        "hooks": [
          {
            "type": "command",
            "command": ".claude/scripts/detect-manual-changes.sh",
            "timeout": 5
          }
        ]
      }
    ],
    "SessionEnd": [
      {
        "hooks": [
          {
            "type": "command",
            "command": ".claude/scripts/save-session.sh",
            "timeout": 60
          }
        ]
      }
    ]
  }
}
