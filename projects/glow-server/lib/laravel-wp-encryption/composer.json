{
  "name": "wonder-planet/laravel-wp-encryption",
  "description": "A Laravel middleware for encrypting and decrypting requests and responses",
  "authors": [
    {
      "name": "Wonder Planet"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "illuminate/http": "^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "orchestra/testbench": "^7 || ^8.5"
  },
  "autoload": {
    "psr-4": {
      "WonderPlanet\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WonderPlanet\\Test\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "WonderPlanet\\EncryptionServiceProvider"
      ]
    }
  }
}
