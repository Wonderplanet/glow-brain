{
  "name": "wonder-planet/laravel-wp-currency",
  "description": "WonderPlanet Laravel Currency Library",
  "authors": [
    {
      "name": "Wonder Planet"
    }
  ],
  "require": {
    "php": "^8.2",
    "laravel/framework": "^11.0",
    "doctrine/dbal": "^3.7",
    "google/apiclient": "^2.15",
    "maatwebsite/excel": "^3.1",
    "mk-j/php_xlsxwriter": "^0.39.0",
    "wonder-planet/laravel-wp-billing": "dev-main",
    "wonder-planet/laravel-wp-common": "dev-main"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "orchestra/testbench": "^7 || ^8.5"
  },
  "autoload": {
    "psr-4": {
      "WonderPlanet\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WonderPlanet\\Test\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "WonderPlanet\\CurrencyServiceProvider"
      ]
    }
  }
}
