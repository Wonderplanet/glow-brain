{
  "name": "wonder-planet/laravel-wp-instrumentation",
  "description": "WonderPlanet Laravel Instrumentation Library",
  "authors": [
    {
      "name": "Wonder Planet"
    }
  ],
  "require": {
    "php": "^8.3",
    "laravel/framework": "^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "orchestra/testbench": "^7 || ^8.5"
  },
  "autoload": {
    "psr-4": {
      "WonderPlanet\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WonderPlanet\\Test\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "WonderPlanet\\InstrumentationServiceProvider"
      ]
    }
  }
}
