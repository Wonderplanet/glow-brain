{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "repositories": [
        {
            "type": "path",
            "url": "/var/local/lib/laravel-wp-billing",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "/var/local/lib/laravel-wp-common",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "/var/local/lib/laravel-wp-currency",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "/var/local/lib/laravel-wp-encryption",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "/var/local/lib/laravel-wp-instrumentation",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "/var/local/lib/laravel-wp-master-asset-release",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^8.3",
        "aws/aws-sdk-php": "^3.334",
        "doctrine/dbal": "^3.7",
        "firebase/php-jwt": "^6.3",
        "google/apiclient": "^2.15",
        "guzzlehttp/guzzle": "^7.4.3",
        "laravel/framework": "^11.0",
        "laravel/slack-notification-channel": "^3.5",
        "laravel/tinker": "^2.7",
        "momentohq/client-sdk-php": "1.17.0",
        "momentohq/momento-php-redis-client": "^0.3.1",
        "symfony/yaml": "^7.1",
        "wonder-planet/laravel-wp-billing": "dev-main",
        "wonder-planet/laravel-wp-common": "dev-main",
        "wonder-planet/laravel-wp-currency": "dev-main",
        "wonder-planet/laravel-wp-encryption": "dev-main",
        "wonder-planet/laravel-wp-instrumentation": "dev-main",
        "wonder-planet/laravel-wp-master-asset-release": "dev-main"
    },
    "require-dev": {
        "fakerphp/faker": "^1.19.0",
        "laravel/pint": "^1.5",
        "laravel/telescope": "^5.2",
        "mockery/mockery": "^1.5.0",
        "nunomaduro/collision": "^8.1",
        "nunomaduro/larastan": "^2.0",
        "phpunit/phpunit": "^10.0",
        "qossmic/deptrac-shim": "^1.0",
        "slevomat/coding-standard": "^8.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Wonderplanet\\": "vendor/wonder-planet/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "WonderPlanet\\Tests\\": [
                "vendor/wonder-planet/laravel-wp-billing/tests/",
                "vendor/wonder-planet/laravel-wp-currency/tests/",
                "vendor/wonder-planet/laravel-wp-common/tests/"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 1200,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
