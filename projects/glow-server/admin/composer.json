{
    "name": "laravel/laravel",
    "type": "project",
    "repositories": [
        {
            "type": "path",
            "url": "../share",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../lib/laravel-wp-encryption",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../lib/laravel-wp-billing",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../lib/laravel-wp-currency",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../lib/laravel-wp-common",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../lib/laravel-wp-master-asset-release",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../lib/laravel-wp-common-admin",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../lib/laravel-wp-master-asset-release-admin",
            "options": {
                "symlink": true
            }
        }
    ],
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "awcodes/filament-tiptap-editor": "^3.0",
        "czproject/git-php": "^4.0.5",
        "filament/filament": "^3.0-stable",
        "google/cloud-storage": "^1.48",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^11.0",
        "laravel/sanctum": "^4.0",
        "laravel/slack-notification-channel": "^3.5",
        "laravel/socialite": "^5.14",
        "laravel/tinker": "^2.7",
        "laravel/ui": "^4.5",
        "league/flysystem-aws-s3-v3": "^3.13",
        "maatwebsite/excel": "^3.1",
        "momentohq/client-sdk-php": "1.17.0",
        "momentohq/momento-php-redis-client": "^0.3.1",
        "spatie/laravel-google-cloud-storage": "^2.3",
        "spatie/laravel-permission": "^6.0",
        "wonder-planet/app-share": "*",
        "wonder-planet/laravel-wp-billing": "dev-main",
        "wonder-planet/laravel-wp-common": "dev-main",
        "wonder-planet/laravel-wp-common-admin": "dev-main",
        "wonder-planet/laravel-wp-currency": "dev-main",
        "wonder-planet/laravel-wp-encryption": "dev-main",
        "wonder-planet/laravel-wp-master-asset-release": "dev-main",
        "wonder-planet/laravel-wp-master-asset-release-admin": "dev-main"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Wonderplanet\\": "vendor/wonder-planet/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "WonderPlanet\\Tests\\": [
                "vendor/wonder-planet/laravel-wp-billing/tests/",
                "vendor/wonder-planet/laravel-wp-currency/tests/",
                "vendor/wonder-planet/laravel-wp-common/tests/"
            ]
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan vendor:publish --tag=wp-admin-files --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "@php artisan vendor:publish --tag=wp-admin-files --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "process-timeout": 1200,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
